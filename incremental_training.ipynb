    "MAX_HOLDOUT = 250\\n",
    "EVAL_SEED = 42\\n",
    "EPS_MSE  = 0.995   # â‰¥ 0.5 % relative MSE improvement\\n",
    "EPS_SSIM = 0.002   # â‰¥ 0.002 absolute SSIM gain (~0.2 %)\\n"
   ]
